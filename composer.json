{
    "name": "muhammadsadeeq/laravel-activitylog-ui",
    "description": "A beautiful, modern UI for Spatie's Activity Log with advanced filtering, analytics, and real-time features.",
    "keywords": [
        "spatie",
        "laravel-activitylog",
        "ui",
        "laravel",
        "livewire",
        "analytics",
        "audit",
        "activity-log"
    ],
    "homepage": "https://github.com/msadeeq/laravel-activitylog-ui",
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammad Sadeeq",
            "email": "afridi.sadeeq.m@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-activitylog": "^4.8",
        "illuminate/contracts": "^10.0|^11.0",
        "livewire/livewire": "^3.0",
        "maatwebsite/excel": "^3.1",
        "barryvdh/laravel-dompdf": "^2.0",
        "pusher/pusher-php-server": "^7.2",
        "laravel/reverb": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.1",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "MuhammadSadeeq\\ActivitylogUi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MuhammadSadeeq\\ActivitylogUi\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MuhammadSadeeq\\ActivitylogUi\\ActivitylogUiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
