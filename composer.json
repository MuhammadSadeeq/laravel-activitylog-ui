{
    "name": "muhammadsadeeq/laravel-activitylog-ui",
    "description": "A beautiful, modern UI for Spatie's Activity Log with advanced filtering, analytics, and real-time features.",
    "keywords": [
        "spatie",
        "laravel-activitylog",
        "activitylog",
        "activity-log",
        "audit",
        "audit-log",
        "laravel",
        "laravel-package",
        "ui",
        "tailwind",
        "alpinejs",
        "chartjs",
        "analytics",
        "dashboard",
        "timeline",
        "export",
        "excel",
        "csv",
        "pdf",
        "logging",
        "monitoring"
    ],
    "homepage": "https://github.com/msadeeq/laravel-activitylog-ui",
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammad Sadeeq",
            "email": "afridi.sadeeq.m@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-activitylog": "^4.8",
        "illuminate/contracts": "^10.0|^11.0|^12.0"
    },
    "suggest": {
        "maatwebsite/excel": "Required for exporting activity logs to XLSX format.",
        "barryvdh/laravel-dompdf": "Required for exporting activity logs to PDF format."
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.1",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "MuhammadSadeeq\\ActivitylogUi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MuhammadSadeeq\\ActivitylogUi\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MuhammadSadeeq\\ActivitylogUi\\ActivitylogUiServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
